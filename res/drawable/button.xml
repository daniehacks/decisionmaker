<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#ff9933" />
            <stroke 
                android:width="1dp"
                android:color="#ff6600"/>
            <corners
                android:radius="6dp"/>
            <padding 
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>
<item>
    <shape>
        <gradient
            android:startColor="#ff9933"
            android:endColor="#ff6600"
            android:angle="270"/>
        <stroke
            android:width="1dp"
            android:color="#ff6600" />
        <corners
            android:radius="6dp" />
        <padding
            android:left="10dp"
            android:top="10dp"
            android:right="10dp"
            android:bottom="10dp"/>
    </shape>
</item>
</selector>
